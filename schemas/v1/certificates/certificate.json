{
    "$id": "Certificate",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "description": "Certificate object",
    "additionalProperties": false,
    "properties": {
        "id": {
            "type": "integer"
        },
        "project": {
            "$ref": "/v1/projects/project.json"
        },
        "hash": {
            "type": "string",
            "maxLength": 100
        },
        "organization_name": {
            "type": "string",
            "maxLength": 2500
        },
        "valid_from": {
            "type": "string",
            "format": "datetime"
        },
        "valid_to": {
            "type": "string",
            "format": "datetime"
        }
    }
}
