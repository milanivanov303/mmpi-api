{
    "$id": "Project",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "description": "Project object",
    "additionalProperties": false,
    "properties": {
        "id": {
            "type": "integer"
        },
        "name": {
            "type": "string",
            "description": "Project name",
            "maxLength": 128,
            "example": "ABSA"
        },
        "clnt_cvs_dir": {
            "type": ["string", "null"],
            "maxLength": 32
        },
        "pnp_type": {
            "type": ["string", "null"],
            "maxLength": 32
        },
        "clnt_code": {
            "type": ["string", "null"],
            "maxLength": 16
        },
        "clnt_code2": {
            "type": ["string", "null"],
            "maxLength": 16
        },
        "src_prefix": {
            "type": ["string", "null"],
            "maxLength": 8
        },
        "src_prefix2": {
            "type": ["string", "null"],
            "maxLength": 8
        },
        "src_itf_prefix": {
            "type": ["string", "null"],
            "maxLength": 8
        },
        "getdcli": {
            "type": ["string", "null"],
            "maxLength": 16
        },
        "getdcli2": {
            "type": ["string", "null"],
            "maxLength": 16
        },
        "activity": {
            "oneOf": [
                {"$ref": "/v1/enum-value.json"},
                {"type": "null"}
            ]
        },
        "activite_gpc": {
            "type": ["string", "null"]
        },
        "activite_sdr": {
            "type": "string",
            "enum": [
                "o",
                "n"
            ]
        },
        "imx_formstag": {
            "type": ["string", "null"],
            "maxLength": 16
        },
        "forms_lng_dlvry": {
            "type": "integer"
        },
        "uses_transl_upd": {
            "type": "integer"
        },
        "inactive": {
            "type": "integer"
        },
        "display_name": {
            "type": "string",
            "maxLength": 128
        },
        "group": {
            "oneOf": [
                {"$ref": "/v1/enum-value.json"},
                {"type": "null"}
            ]
        },
        "country": {
            "oneOf": [
                {"$ref": "/v1/enum-value.json"},
                {"type": "null"}
            ]
        },
        "communication_lng": {
            "oneOf": [
                {"$ref": "/v1/enum-value.json"},
                {"type": "null"}
            ]
        },
        "delivery_method": {
            "oneOf": [
                {"$ref": "/v1/enum-value.json"},
                {"type": "null"}
            ]
        },
        "se_mntd_by_clnt": {
            "oneOf": [
                {"$ref": "/v1/enum-value.json"},
                {"type": "null"}
            ]
        },
        "tl_mntd_by_clnt": {
            "oneOf": [
                {"$ref": "/v1/enum-value.json"},
                {"type": "null"}
            ]
        },
        "njsch_mntd_by_clnt": {
            "oneOf": [
                {"$ref": "/v1/enum-value.json"},
                {"type": "null"}
            ]
        },
        "trans_mntd_by_clnt": {
            "oneOf": [
                {"$ref": "/v1/enum-value.json"},
                {"type": "null"}
            ]
        },
        "sla_from": {
            "type": ["string", "null"],
            "format": "time"
        },
        "sla_to": {
            "type": ["string", "null"],
            "format": "time"
        },
        "modified_by": {
            "oneOf": [
                {"$ref": "/v1/user.json"},
                {"type": "null"}
            ]
        },
        "type_business": {
            "oneOf": [
                {"$ref": "/v1/enum-value.json"},
                {"type": "null"}
            ]
        },
        "delivery_chains": {
            "type": "array",
            "items": {
                "$ref": "/v1/delivery-chains/delivery-chain.json"
            }
        }
    }
}
