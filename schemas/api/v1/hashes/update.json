{
    "$id": "/api/v1/hashes/update.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "description": "Update Hash",
    "required": [
        "branch",
        "owner"
    ],
    "properties": {
        "branch": {
            "$ref": "/api/v1/hashes/hash.json#/properties/branch"
        },
        "owner": {
            "$ref": "/api/v1/hashes/hash.json#/properties/owner"
        },
        "chains": {  
           "$ref": "/api/v1/hashes/hash.json#/properties/chains"
        },
        "description": {
            "$ref": "/api/v1/hashes/hash.json#/properties/description"
        },
        "files": {
            "$ref": "/api/v1/hashes/hash.json#/properties/files"
        },
        "merge_branch": {
            "$ref": "/api/v1/hashes/hash.json#/properties/merge_branch"
        },
        "module": {
            "$ref": "/api/v1/hashes/hash.json#/properties/module"
        },
        "repo_path": {
            "$ref": "/api/v1/hashes/hash.json#/properties/repo_path"
        },
        "repo_url": {
            "$ref": "/api/v1/hashes/hash.json#/properties/repo_url"
        }
    }
}
