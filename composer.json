{
    "name": "mmpi api",
    "description": "The API for MMPI tool.",
    "keywords": ["mmpi", "api", "REST", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "laravel/lumen-framework": "5.7.*",
        "vlucas/phpdotenv": "~2.2",
        "predis/predis": "^1.1",
        "illuminate/redis": "^5.6",
        "opis/json-schema": "^1.0",
        "symfony/psr-http-message-bridge": "^1.1",
        "zendframework/zend-diactoros": "^2.0",
        "adldap2/adldap2": "^9.1",
        "firebase/php-jwt": "^5.0",
        "elasticsearch/elasticsearch": "^6.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpmd/phpmd": "^2.6",
        "doctrine/dbal": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "Core/",
            "App\\": "App/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "test": "phpunit --colors=always",
        "test:unit": "phpunit --colors=always ./tests/Unit",
        "test:functional": "phpunit --colors=always ./tests/Functional",
        "test:unit:coverage": "phpunit --coverage-text --colors=never ./tests/Unit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
